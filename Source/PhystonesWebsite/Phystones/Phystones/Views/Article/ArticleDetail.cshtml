@using EntitiesObject.Entities.WebEntities
@{
    ViewBag.Title = Model.Title;
    Layout = "~/Views/Shared/_Layout_Web.cshtml";
}
@model Out_Article_GetByTextId_Result
@if (Model != null)
{
    <hr class="grid-2 center mq-mobile">
    <div class="grid-10 center" id="terms">
        <h1 id="tagline">@Model.Title</h1>
    </div>
    <section class="section text-left">
        @Html.Raw(Model.ContentBody)
    </section>

}

