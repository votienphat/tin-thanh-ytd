@using EntitiesObject.Entities.WebEntities
@{
    ViewBag.Title = WebContent.Contact;
    Layout = "~/Views/Shared/_Layout_Web.cshtml";
    var content = ViewBag.Content as Out_Slogan_GetById_Result;
}
@using Phystones.Content.Texts
@using Phystones.Models.ContentData
@model BusinessObject.WebModule.Models.Config.ContactConfigModel
@if (content != null)
{
    <h1 id="tagline">@content.Title</h1>
    <h2 class="grid-10 center">
        @Html.Raw(content.ContentBody)
    </h2>
}

<section class="section grid-12 center" id="saveContact">
    @Html.Partial("SendMess",new SendContact())
</section>

